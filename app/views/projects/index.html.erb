<h1>All Projects</h1>
<table cellpadding="10" border="1">
  <thead>
    <tr>
      <th>Project Name</th>
      <th>Total Project Size</th>
    </tr>
  </thead>
  <tbody>
    <% @projects.each do |project| %>
      <tr class="project-row" id="<%= dom_id(project) %>">
        <td class="name"><%= project.name %></td>
        <td class="total-size"><%= project.total_size %></td>
      </tr>
    <% end %>
  </tbody>
</table>
