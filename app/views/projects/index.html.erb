<h1>All Projects</h1>
<table cellpadding="10" border="1">
  <thead>
    <tr>
      <th>Project Name</th>
      <th>Total Project Size</th>
    </tr>
  </thead>
  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.name %></td>
        <td><%= project.total_size %></td>
      </tr>
    <% end %>
  </tbody>
</table>
